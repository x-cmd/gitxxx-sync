
git clone git@github.com:x-cmd/yanfa
cd yanfa

function go(){
    x job failfast init
    while read -r owner_repo; do
        # x job failfast put x gitb backup "git@gitee.com:x-bash/$repo" "git@github.com:x-bash/$repo"
        x job failfast put x gitb backup "git@github.com:$owner_repo" "git@gitee.com:$owner_repo"
        # TODO: codeup
    done | (
        x job failfast fini
    )
}

(

xws xx x0 | while read -r r; do
    printf "x-bash/%s\n" "$r"
done | go

xws x-cmd | while read -r r; do
    printf "x-cmd/%s\n" "$r"
done | go

# sync organization: x-cmd-build, x-cmd-builder, x-cmd-advise, x-cmd-osiso

)
