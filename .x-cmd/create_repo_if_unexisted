

create_repo_if_unexisted(){
    local org="$1"
    # x gh repo ls "$org" | x line minus "$(x gt repo ls "$org")" | while read -r line; do
    #     x gt repo create "$org/$line"
    # done

    x minus "$(x gt repo ls "$org")" | x prf "$org/\n" | x gt repo create
}

create_repo_if_unexisted "$@"

# x yanfa vstream x0          |   create_repo_if_unexisted x-bash
# x yanfa vstream x-cmd       |   create_repo_if_unexisted x-cmd
# x gh repo ls x-cmd-osiso    |   create_repo_if_unexisted x-cmd-osiso
# x gh repo ls x-cmd-build    |   create_repo_if_unexisted x-cmd-build
# x gh repo ls x-cmd-builder  |   create_repo_if_unexisted x-cmd-builder

