

gt_create_repo_if_unexisted(){
    local org="$1"
    x minus "$(x gt repo ls "$org")" | x prf "$org/\n" | x arg1 'x gt repo create $org/$1'
}

gh_create_repo_if_unexisted(){
    local org="$1"
    x minus "$(x gh repo ls "$org")" | x prf "$org/\n" | x arg1 'x gh repo create $1'
}

gcode_create_repo_if_unexisted(){
    local org="$1"
    x minus "$(x gcode repo ls "$org")" | x prf "$org/\n" | x arg1 'x gcode repo create $1'
}

gt_create_repo_if_unexisted "$@"

# x yanfa vstream x0          |   create_repo_if_unexisted x-bash
# x yanfa vstream x-cmd       |   create_repo_if_unexisted x-cmd
# x gh repo ls x-cmd-osiso    |   create_repo_if_unexisted x-cmd-osiso
# x gh repo ls x-cmd-build    |   create_repo_if_unexisted x-cmd-build
# x gh repo ls x-cmd-builder  |   create_repo_if_unexisted x-cmd-builder

